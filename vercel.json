{
    "version": 2,
    "builds": [
      {
        "src": "backend/main.py",
        "use": "@vercel/python",
        "config": {
          "maxDuration": 10
        }
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.9"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/main.py"
      }
    ]
  }
  